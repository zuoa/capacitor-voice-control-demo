{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "@capacitor/core",
      "react",
      "react-dom"
    ],
    "jsx": "react-jsx",
    "lib": [
      "ES2020",
      "DOM"
    ]
  },
  "include": [
    "src",
    "vite-env.d.ts"
  ]
}